<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.business.entity">
	<class
		name="Client"
		table="client"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="Number"
			column="number"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Contact"
			column="contact"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Unit"
			column="unit"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Web"
			column="web"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="Nationality"
			column="nationality"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Remark"
			column="remark"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Reserved"
			column="reserved"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Saleman"
			column="saleman"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="Clientfrom"
			column="clientfrom"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Jointime"
			column="jointime"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="Potential"
			column="potential"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Havequote"
			column="havequote"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Mailingsample"
			column="mailingsample"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Potentialreserved1"
			column="potentialreserved1"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Potentialreserved2"
			column="potentialreserved2"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Potentialreserved3"
			column="potentialreserved3"
			type="java.lang.Double"
			not-null="false"
			length="22"
		/>


	</class>	
</hibernate-mapping>