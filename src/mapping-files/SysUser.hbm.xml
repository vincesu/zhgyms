<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.framework.entity">
	<class
		name="SysUser"
		table="sys_user"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="uid"
		>
			<generator class="increment"/>
		</id>

		<property
			name="Username"
			column="username"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="Pwd"
			column="pwd"
			type="string"
			not-null="true"
			length="80"
		/>
		<property
			name="RoleId"
			column="role_id"
			type="java.lang.Long"
			not-null="true"
			length="19"
		/>
		<property
			name="Reserved"
			column="reserved"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="JoinedTime"
			column="joinedTime"
			type="date"
			not-null="true"
			length="10"
		/>
		<property
			name="LastLoginTime"
			column="lastLoginTime"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="Realname"
			column="realname"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="PaymentEn"
			column="payment_en"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="PaymentCn"
			column="payment_cn"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Shippingtrem"
			column="shippingtrem"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Fax"
			column="fax"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="ContractNote"
			column="contract_note"
			type="string"
			not-null="false"
		/>
		<property
			name="ContractNoteCn"
			column="contract_note_cn"
			type="string"
			not-null="false"
		/>


	</class>	
</hibernate-mapping>