<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.business.entity">
	<class
		name="Contract"
		table="contract"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="BuyerId"
			column="buyer_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="OrderId"
			column="order_id"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Number"
			column="number"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="OrderDate"
			column="order_date"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="DeliveryDate"
			column="delivery_date"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Saleman"
			column="saleman"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="Reserved"
			column="reserved"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="LeadTime"
			column="lead_time"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ShippingTrem"
			column="shipping_trem"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Payment"
			column="payment"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Remark"
			column="remark"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CurrencySymbol"
			column="currency_symbol"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="IsAudit"
			column="is_audit"
			type="boolean"
			not-null="false"
		/>
		<property
			name="Audit"
			column="audit"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="Tel"
			column="tel"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Fax"
			column="fax"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Createc"
			column="createc"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Makepoint"
			column="makepoint"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="Complete"
			column="complete"
			type="string"
			not-null="false"
			length="5"
		/>
	</class>	
</hibernate-mapping>