<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.business.entity">
	<class
		name="Suppliers"
		table="suppliers"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="Number"
			column="number"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Contact"
			column="contact"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Unit"
			column="unit"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="MainItems"
			column="main_items"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ContactWay"
			column="contact_way"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Address"
			column="address"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="Bank"
			column="bank"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="BankAccount"
			column="bank_account"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="BankAccountName"
			column="bank_account_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Reserved"
			column="reserved"
			type="string"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>