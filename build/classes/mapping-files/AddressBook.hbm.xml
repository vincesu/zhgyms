<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.business.entity">
	<class
		name="AddressBook"
		table="address_book"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Gender"
			column="gender"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="OfficePhone"
			column="office_phone"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="Email"
			column="email"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Number"
			column="number"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Department"
			column="department"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Posi"
			column="posi"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Birthday"
			column="birthday"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="GraduateSchool"
			column="graduate_school"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="EducationalBackground"
			column="educational_background"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="EntryDate"
			column="entry_date"
			type="date"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>