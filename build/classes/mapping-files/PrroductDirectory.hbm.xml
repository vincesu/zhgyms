<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.business.entity">
	<class
		name="PrroductDirectory"
		table="prroduct_directory"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Number"
			column="number"
			type="string"
			not-null="false"
			length="100"
		/>


	</class>	
</hibernate-mapping>