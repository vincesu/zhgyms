<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vincesu.business.entity">
	<class
		name="MaterialsInformation"
		table="materials_information"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="java.lang.Long"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="Number"
			column="number"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Name"
			column="name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Size"
			column="size"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Price"
			column="price"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Manufacturers"
			column="manufacturers"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Contact"
			column="contact"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Phone"
			column="phone"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Category"
			column="category"
			type="string"
			not-null="false"
			length="50"
		/>

	</class>	
</hibernate-mapping>